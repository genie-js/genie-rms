  },
  "devDependencies": {
    "brfs": "^2.0.1",
    "buble": "^0.19.3",
    "jest": "^23.6.0",
    "standard": "^11.0.1"
  },
  "keywords": [
    "age of empires",
    "genie",
    "random map",
    "random map script",
    "rms"
  ],
  "license": "GPL-3.0",
  "main": "lib",
  "repository": "goto-bus-stop/genie-rms",
  "scripts": {
    "build": "buble src --output lib -y dangerousForOf -m && npm run build:json && npm run build:lres && npm run build:rmdef",
    "build:json": "cp src/*.json lib/",
    "build:lres": "brfs src/landResources.js > lib/landResources.js",
    "build:rmdef": "brfs src/randomMapDef.js > lib/randomMapDef.js",
    "test": "jest"
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
